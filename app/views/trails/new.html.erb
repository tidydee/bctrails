<%= form_for @trail do |f| %>
  
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :street %>:
  <%= f.text_field :street %><br />

  <%= f.label :city %>:
  <%= f.text_field :city %><br />

  <%= f.label :postal_code %>:
  <%= f.text_field :postal_code %><br />

    
  <%= f.submit %>


<% end %> 